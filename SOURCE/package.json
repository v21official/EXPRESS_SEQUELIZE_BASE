    "hat": "^0.0.3",
    "http-errors": "~1.6.3",
    "module-alias": "^2.2.2",
    "morgan": "~1.9.1",
    "mysql2": "^2.1.0",
    "pug": "2.0.0-beta11",
    "sequelize": "^5.21.7"
  },
  "devDependencies": {
    "nodemon": "^2.0.3"
  },
  "_moduleAliases": {
    "@src": "./src",
    "@controllers": "./src/controllers",
